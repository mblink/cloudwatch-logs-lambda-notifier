{
  "name": "cloudwatch-logs-notify-lambda",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "clean": "rm -rf {build,dist} && mkdir dist",
    "compile": "babel src -d dist",
    "deploy": "npm run clean && npm run update-geoip && npm run compile && node-lambda deploy -e production",
    "lint": "eslint --config .eslintrc.js src test",
    "local": "npm run compile && node-lambda run",
    "postinstall": "node-lambda setup",
    "test": "mocha --require babel-core/register --reporter mocha-better-spec-reporter --recursive",
    "update-geoip": "node node_modules/geoip-lite/scripts/updatedb.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mblink/cloudwatch-logs-lambda-notifier.git"
  },
  "author": "BondLink <dev@bondlink.org>",
  "license": "Apache-2.0",
  "dependencies": {
    "@sendgrid/mail": "~6.3.1",
    "aws-sdk": "~2.388.0",
    "geoip-lite": "~1.3.5",
    "he": "~1.2.0",
    "lambda-state": "github:mblink/lambda-state#0.0.1"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-preset-env": "~1.7.0",
    "chai": "~4.2.0",
    "dirty-chai": "~2.0.1",
    "eslint": "~5.12.0",
    "eslint-config-airbnb": "~17.1.0",
    "eslint-plugin-import": "~2.14.0",
    "mocha": "~6.0.0-1",
    "mocha-better-spec-reporter": "~3.1.0",
    "node-lambda": "~0.13.0",
    "sinon": "~7.2.2",
    "sinon-chai": "~3.3.0"
  }
}
