  "private": true,
  "dependencies": {
    "aws-sdk": "^2.30.0",
    "sendgrid": "^4.8.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "node-lambda": "^0.8.13"
  }
}
